{
  "name": "hm-delbestilling",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2"
  },
  "scripts": {
    "install:client": "yarn --cwd client install",
    "install:server": "yarn --cwd server install",
    "postinstall": "run-p install:*",
    "clean:client": "yarn --cwd client clean",
    "clean:server": "yarn --cwd server clean",
    "clean": "run-p clean:*",
    "dev:client": "yarn --cwd client dev",
    "dev:server": "yarn --cwd server dev",
    "dev:docker": "yarn --cwd server dev:docker",
    "dev": "yarn dev:client",
    "build:client": "yarn --cwd client build",
    "build:server": "yarn --cwd server build",
    "build": "run-p build:*"
  }
}
